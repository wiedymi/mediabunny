<?xml version="1.0" encoding="utf-8"?>
<tt
  xmlns="http://www.w3.org/ns/ttml"
  xmlns:ttm="http://www.w3.org/ns/ttml#metadata"
  xmlns:tts="http://www.w3.org/ns/ttml#styling"
  xmlns:ttp="http://www.w3.org/ns/ttml#parameter"
  ttp:cellResolution="384 288"
  xml:lang="">
  <head>
    <layout>
      <region xml:id="Default"
        tts:origin="3% 0%"
        tts:extent="97% 97%"
        tts:displayAlign="after"
        tts:textAlign="center"
        tts:fontSize="16c"
        tts:fontFamily="Arial"
        tts:overflow="visible" />
    </layout>
  </head>
  <body>
    <div>
      <p
        begin="00:00:01.000"
        end="00:00:03.500"><span region="Default">Hello world!</span></p>
      <p
        begin="00:00:05.000"
        end="00:00:07.000"><span region="Default">This is a test.</span></p>
      <p
        begin="00:00:08.500"
        end="00:00:10.000"><span region="Default">Goodbye!</span></p>
    </div>
  </body>
</tt>
